{
	"type": "class",
	"name": "UserClass",
	"export": "LIB_ANIREM_EXPORT",
	"includes": [
		">lib_anirem_global.h",
		">proxerlist.h",
		">proxerlogin.h"
	],

	"path": "user",
	"except": "ProxerStatus",

	"methods": {
		"login": {
			"path": "login",
			"verb": "POST",
			"parameters": [
				"username;QString",
				"password;QString"
			],
			"returns": "ProxerLogin"
		},
		"login2FA": {
			"path": "login",
			"verb": "POST",
			"parameters": [
				"username;QString",
				"password;QString",
				"secretkey;QString"
			],
			"returns": "ProxerLogin"
		},

		"listEntries": {
			"path": "list",
			"parameters": [
				"kat;QString",
				"isH;bool;false",
				"p;int;0",
				"limit;int;100",
				"sort;QString;changeDateDESC",
				"filter;QString;stateFilter0"
			],
			"returns": "ProxerList"
		}
	}
}

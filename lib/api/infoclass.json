	"includes": [
		">lib_anirem_global.h",
		">proxerentry.h",
		">proxerrelations.h"
	],

	"path": "info",
	"except": "ProxerStatus",

	"methods": {
		"getEntry": {
			"path": "entry",
			"parameters": [
				"id;int"
			],

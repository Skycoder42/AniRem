	<Include>QSize</Include>

	<Entry key="migrated" type="bool" default="false"/>
	<Entry key="didNotify" type="bool" default="false"/>
	<Node key="gui">
		<Node key="mainwindow">
			<Entry key="geom" type="QByteArray"/>
			<Entry key="state" type="QByteArray"/>
			<Entry key="header" type="QByteArray"/>
		</Node>
	</Node>
	<Node key="account">
		<Entry key="username" type="QString"/>
		<Entry key="storedPw" type="bool" default="false"/>
		<Entry key="token" type="QString"/>
	</Node>
</Settings>
